{
  "env": {
    "browser": true,
      "node": true
  },
  "rules": {
    "eqeqeq": [2, "smart"],
    "strict": [2, "global"],
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "quotes": [2, "single"],
    "curly": [2, "multi-line"],
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-shadow": 0,
    "consistent-return": 0
  }
}
